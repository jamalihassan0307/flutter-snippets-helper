{
  "name": "flutter-snippets-helper",
  "displayName": "Flutter Snippets Helper",
  "description": "Essential Flutter widget snippets for rapid development",
  "version": "0.0.4",
  "publisher": "7jsscmp4zaio626xj6rxx77zhgeosa4yry5vhbr3hu7gcgt4k73q",
  "engines": {
    "vscode": "^1.93.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamalihassan0307/flutter-snippets-helper"
  },
  "license": "MIT",
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "flutter",
    "dart",
    "snippets",
    "flutter widgets"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/flutter-snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "icon": "images/logo.png"
}
